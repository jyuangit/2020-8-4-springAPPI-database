<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.employee.restfulapi.mapper.CompanyMapper">


    <update id="updateCompanyById" parameterType="com.example.employee.restfulapi.entity.Company">
        update company
        set
            companyName=#{companyName},
            employeesNumber = #{employeesNumber}
        where id = #{id}
    </update>







</mapper>
